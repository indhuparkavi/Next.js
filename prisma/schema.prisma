generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String
  addressId Int
  address Address @relation(fields: [addressId], references: [id])
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  skills String[]
}

model Address {
  id      Int    @id @default(autoincrement())
  st      String
  city    String
  country String
  users   User[]
}

model Skill {
  id Int    @id @default(autoincrement())
  name String
}
